{
    "name": "mark-a-j-adriano/silverstripe-addy-finder",
    "type": "silverstripe-vendormodule",
    "description": "SilverStripe FormField for https://www.addy.co.nz/",
    "keywords": [
        "silverstripe",
        "form"
    ],
    "homepage": "https://github.com/mark-a-j-adriano/silverstripe-addy-finder",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mark Anthony Adriano",
            "email": "dev@heyday.co.nz",
            "homepage": "https://www.example.io"
        }
    ],
    "require": {
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "mark-a-j-adriano\\SilverStripe\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
